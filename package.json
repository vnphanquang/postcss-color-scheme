{
  "name": "postcss-color-scheme",
  "version": "1.0.1",
  "description": "postcss plugin for handling prefers-color-scheme",
  "type": "module",
  "main": "lib/postcss.js",
  "types": "types/postcss.d.ts",
  "exports": {
    ".": {
      "import": "./lib/postcss.js",
      "require": "./lib/postcss.js",
      "types": "./types/postcss.d.ts"
    },
    "./postcss": {
      "import": "./lib/postcss.js",
      "require": "./lib/postcss.js",
      "types": "./types/postcss.d.ts"
    },
    "./tailwind": {
      "import": "./lib/tailwind.js",
      "require": "./lib/tailwind.js",
      "types": "./types/tailwind.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "postcss": [
        "./types/postcss.d.ts"
      ],
      "tailwind": [
        "./types/tailwind.d.ts"
      ]
    }
  },
  "scripts": {
    "dts": "tsc",
    "prepublishOnly": "tsc",
    "lint": "eslint --ignore-path .gitignore \"./**/*/*{ts,js,cjs}\"",
    "test": "vitest --ui --coverage",
    "ci:test": "vitest run --coverage",
    "ci:version": "changeset version && pnpm install --no-frozen-lockfile",
    "ci:publish": "changeset publish && pnpm install",
    "format": "prettier --plugin-search-dir . --write ."
  },
  "files": [
    "lib/index.js",
    "lib/color-scheme.js",
    "lib/tailwind.js",
    "types",
    "CHANGELOG.md",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "author": "Quang Phan",
  "license": "MIT",
  "keywords": [
    "postcss-plugin",
    "postcss",
    "plugin",
    "theme",
    "dark",
    "color",
    "scheme",
    "mode"
  ],
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.1",
    "@types/node": "^20.11.16",
    "@types/postcss-js": "^4.0.4",
    "@vitest/coverage-istanbul": "^1.2.2",
    "@vitest/ui": "^1.2.2",
    "@vnphanquang/eslint-config": "^2.0.0",
    "@vnphanquang/prettierrc": "^1.0.1",
    "postcss": "^8.4.33",
    "postcss-js": "^4.0.1",
    "postcss-nested": "^6.0.1",
    "postcss-nesting": "^12.0.2",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  },
  "peerDependencies": {
    "postcss": "^8.0.0"
  },
  "engines": {
    "pnpm": ">=8.6.0",
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@8.6.7",
  "volta": {
    "node": "20.10.0"
  }
}
