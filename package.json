  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint --ignore-path .gitignore \"./**/*/*{ts,js,cjs}\"",
    "test": "vitest --ui --coverage",
    "ci:test": "vitest run --coverage",
    "ci:version": "changeset version && pnpm install",
    "ci:publish": "changeset publish",
    "format": "prettier --plugin-search-dir . --write ."
  },
  "files": [
    "lib/index.js",
    "lib/color-scheme.js",
    "lib/tailwind.js",
    "CHANGELOG.md",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "author": "Quang Phan",
  "license": "MIT",
  "keywords": [
    "postcss-plugin",
    "postcss",
    "plugin",
    "theme",
    "dark",
    "color",
    "scheme",
    "mode"
  ],
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.2",
    "@types/postcss-js": "^4.0.1",
    "@vitest/coverage-istanbul": "^0.33.0",
    "@vitest/ui": "^0.33.0",
    "@vnphanquang/eslint-config": "^2.0.0",
    "@vnphanquang/prettierrc": "^1.0.1",
    "postcss": "^8.4.26",
    "postcss-js": "^4.0.1",
    "postcss-nested": "^6.0.1",
    "postcss-nesting": "^12.0.0",
    "prettier": "^3.0.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6",
    "vitest": "^0.33.0"
  },
  "peerDependencies": {
    "postcss": "^8.0.0"
  },
  "engines": {
    "pnpm": ">=8.6.0",
    "node": ">=18.16.0"
